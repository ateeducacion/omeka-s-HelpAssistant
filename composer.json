{
    "name": "ateeducacion/omeka-s-helpassistant",
    "description": "Provides contextual help tours for the Omeka S admin interface using Intro.js",
    "type": "omeka-s-module",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Área de Tecnología Educativa",
            "email": "ate.educacion@gobiernodecanarias.org"
        }
    ],
    "homepage": "https://github.com/ateeducacion/omeka-s-ModuleTemplate",
    "support": {
        "issues": "https://github.com/ateeducacion/omeka-s-ModuleTemplate/issues"
    },
    "keywords": [
        "Omeka S",
        "module",
        "3D",
        "three.js",
        "model-viewer"
    ],
    "require": {
        "php": "^8.0"
    },
    "config": {
        "allow-plugins": {
            "omeka/composer-addon-installer": true
        },
        "platform-check": false,
        "platform": {
            "ext-intl": "1.0.0"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "squizlabs/php_codesniffer": "*",
        "phpunit/phpunit": "*",
        "zerocrates/extract-tagged-strings": "dev-master",
        "laminas/laminas-form": "^3.4",
        "laminas/laminas-eventmanager": "^3.5",
        "laminas/laminas-view": "^2.20",
        "laminas/laminas-mvc": "^3.2",
        "omeka/omeka-s": "^4.0"
    },
    "replace": {
        "omeka-s-themes/default": "*"
    },
    "autoload": {
        "psr-4": {
            "HelpAssistant\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HelpAssistantTest\\": "test/"
        }
    },
    "repositories": [
        {
        "type": "vcs",
        "url": "https://github.com/zerocrates/extract-tagged-strings.git"
        }
    ]
}
